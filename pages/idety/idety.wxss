/* pages/idety/idety.wxss */
page{
  background: #fff;
}
.places{
  font-size: 30rpx;
  color: #ccc;
}
.ipts input{
  font-size: 30rpx;
  color: #333;
  padding: 0 20rpx;
  width: 690rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin:0 auto;
}
.margins{
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}
.btns{
  width: 100%;
  margin: 0 auto;
  margin-top: 150rpx;
}
.btns button{
  margin: 0 auto;
  width: 690rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #ff9812;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  border-radius: 10rpx;
  text-align: center;
  padding: 0;
}
.toppic{
  width: 220rpx;
  height: 150rpx;
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 50rpx;
  display: block;
}